<MudDialog>
    <DialogContent>
        <MudTextField T="string" Label="Username" Variant="Variant.Filled" @bind-Value="model.Username">
        </MudTextField>
        <MudTextField T="string" InputType="MudBlazor.InputType.Password" Label="Password" Variant="Variant.Filled" @bind-Value="model.Password">
        </MudTextField>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>

