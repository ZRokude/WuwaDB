<MudDialog>
    <DialogContent>
        <MudAutocomplete T="string" Label="Description Title" SearchFunc="SearchDescTitle" 
            @bind-Value="CharacterSkillDescription.DescriptionTitle" Immediate="true" CoerceValue="false" CoerceText="false" ResetValueOnEmptyText="true"/>
        <MudTextField T="string" AutoGrow="true" Label="Description" @bind-Value="CharacterSkillDescription.Description" Immediate="true"/>
    </DialogContent>
    <DialogActions>
        <MudButton StartIcon="@Icons.Material.Filled.Save" OnClick="Save">
            Save
        </MudButton>
    </DialogActions>
</MudDialog>

